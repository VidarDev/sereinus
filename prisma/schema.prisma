generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Crisis {
  userId   String
  datetime DateTime
  duration Int
  note     String?

  @@id([userId, datetime])
}
